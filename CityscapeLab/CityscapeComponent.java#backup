import java.awt.Graphics;
import java.awt.Graphics2D;
import java.awt.Color;
import javax.swing.JComponent;
import java.awt.Rectangle;
import java.awt.geom.Ellipse2D;
public class CityscapeComponent extends JComponent
{
    public void paintComponent(Graphics g)
    {
            Graphics2D g2 = (Graphics2D) g;
            Rectangle building = new Rectangle(20,400,150,400);
            Rectangle window = new Rectangle(30,410,50,50);
            Rectangle window1 = new Rectangle(110,410,50,50);
            Rectangle window2 = new Rectangle(30,470,50,50);
            Rectangle window3 = new Rectangle(110,470,50,50);
            Rectangle window4 = new Rectangle(30,530,50,50);
            Rectangle window5 = new Rectangle(110,530,50,50);
            Rectangle sky = new Rectangle(0,0,800,800);
            g2.setColor(Color.BLUE);
            g2.fill(sky);
            g2.setColor(Color.BLACK);
            g2.fill(building);
            g2.setColor(Color.WHITE);
            g2.fill(window);
            g2.fill(window1);
            g2.fill(window2);
            g2.fill(window3);
            g2.fill(window4);
            g2.fill(window5);
    }
}
